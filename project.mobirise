              "word-break": "break-word"
            },
            ".card-wrapper": {
              "overflow": "hidden"
            },
            ".card-wrapper > *:not(.mbr-overlay), .card-wrapper": {
              "position": "relative"
            },
            ".card-wrapper when not (@transparentBg)": {
              ".mbr-overlay": {
                "background-color": "@cardBg"
              },
              ".card-head": {
                "background-color": "fade(@secondaryColor, 12%)"
              }
            },
            ".card-wrapper when (@showBorder)": {
