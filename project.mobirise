        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".form-container, .first-element, .first-block": {
              "width": "100%"
            },
            ".form-container, .form-wrap, .form-box, .form-block": {
              "position": "relative"
            },
            ".card .card-wrapper": {
              "flex-direction": "row"
            },
            ".mbr-section-btn": {
              "padding-bottom": "0"
            },
            ".field-input": {
              "color": "@labelsColor",
              "&:not([type=\"checkbox\"]):not([type=\"radio\"])": {
                "background": "contrast(@labelsColor, darken(@labelsColor, 50%), #ffffff, 60%) !important",
                "border": "1px solid lighten(@labelsColor, 88%)",
                "&:focus": {
                  "border-color": "@secondaryColor"
                }
              }
            },
            "input::-webkit-input-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "input::-moz-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "input::placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "textarea::-webkit-input-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "textarea::-moz-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "textarea::placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            ".card-wrapper when not (@transparentBg) and (@showTeam)": {
              "background-color": "@cardBg"
            },
            ".card-wrapper when (@showBorders) and (@showTeam)": {
              "border": "1px solid @bordersColor"
            },
            ".card-subtitle:before when (@lineSpecialty) and (@showTeam)": {
              "content": "''",
              "width": "25px",
              "height": "3px",
              "display": "inline-block",
              "vertical-align": "middle",
              "margin-right": "10px",
              "background-color": "@primaryColor"
            },
            "@media (max-width: 767px)": {
              ".card:not(.last-child) when (@showTeam)": {
                "padding-bottom": "20px"
              },
              ".mbr-jc-e": {
                "justify-content": "center"
              }
            },
            "@media (max-width: 991px)": {
              ".first-element": {
                "padding-bottom": "30px"
              }
            },
            ".card-img when (@showName), (@showSpecialty)": {
              "margin-right": "30px",
              "& amp-img, &": {
                "width": "~\"@{imageWidth}rem\"",
                "height": "auto",
                "@media (max-width: 767px)": {
                  "max-width": "80px"
                }
              }
            },
            ".marker-wrap:before when (@showFormList)": {
              "content": "\"\"",
              "display": "inline-block",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial",
              "& when ((@widthImg * 16) > 40)": {
                "@media (max-width: 767px)": {
                  "width": "40px",
                  "height": "40px"
                }
              }
            },
            ".mbr-list .items-list, .contacts-box .mbr-flex, .card > .mbr-flex": {
              "flex-direction": "row"
            },
            ".mbr-text": {
              "width": "auto"
            },
            ".marker-wrap:before, .contacts-box .form-icon": {
              "margin-right": "15px"
            },
            ".mbr-list .items-list when (@showFormList)": {
              "align-items": "baseline",
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".form-icon .amp-iconfont when (@showIcons)": {
              "font-size": "~\"@{iconsSize}rem\"",
              "width": "~\"@{iconsSize}rem\"",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "30px",
                  "width": "30px"
                }
              }
            },
            ".social .iconfont-wrapper when (@showSocial)": {
              "line-height": "~\"calc(@{socialSize}rem + 4px)\"",
              "width": "~\"@{socialSize}rem\"",
              "height": "~\"@{socialSize}rem\"",
              "& when ((@socialSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "line-height": "34px",
                  "width": "30px",
                  "height": "30px"
                }
              }
            },
            ".social .amp-iconfont when (@showSocial)": {
              "font-size": "~\"calc(@{socialSize}rem * 0.42)\"",
              "width": "~\"calc(@{socialSize}rem * 0.42)\"",
              "line-height": "~\"calc(@{socialSize}rem - 4px)\"",
              "& when ((@socialSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "~\"calc(30px * 0.42)\"",
                  "width": "~\"calc(30px * 0.42)\"",
                  "line-height": "26px"
                }
              }
            },
            ".form-block when (@transparentForm)": {
              "padding": "0"
            },
            ".form-wrap .mbr-overlay when not (@transparentForm)": {
              "background-color": "@formBgColor",
              "opacity": "@formBgOpacity"
            },
            ".mbr-row .card when not (@showPadding)": {
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "0",
                "& when (@showBorders)": {
                  ".card-wrapper": {
                    "border": "none"
                  },
                  "@media (min-width: 768px)": {
                    "&:not(.last-child) .card-wrapper": {
                      "border-right": "1px solid @bordersColor"
                    },
                    "&.mbr-pb-4 .card-wrapper": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.mbr-pb-4:nth-child(2) .card-wrapper": {
                      "border-right": "none"
                    }
                  },
                  "@media (max-width: 767px)": {
                    "&:not(.last-child) .card-wrapper": {
                      "border-right": "none",
                      "border-bottom": "1px solid @bordersColor"
                    }
                  }
                }
              }
            },
            ".title-wrap .mbr-section-title": {
