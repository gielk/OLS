              "width": "~\"@{iconsSize}rem\"",
              "color": "@iconsColor",
              "& when ((@iconsSize * 16) > 40)": {
                "@media (max-width: 767px)": {
                  "font-size": "40px",
                  "width": "40px"
                }
              }
            },
            ".card-price-wrap": {
              "align-items": "center",
              ".card-price": {
                "align-items": "flex-start"
              }
            },
            ".card when not (@transparentBg), (@showBorder)": {
              ".card-head": {
                "padding": "45px 35px 30px",
                "@media (max-width: 991px)": {
                  "padding": "25px 20px 20px"
                }
              },
              ".list-item": {
                "border-bottom": "1px solid",
                "& when not (@transparentBg)": {
                  "border-color": "darken(@cardBg, 9.2%)"
                },
                "& when (@showBorder)": {
                  "border-color": "@colorBorder"
                }
              },
              "&.md-pb when (@cardsAmount = 4)": {
                "@media (max-width: 991px)": {
                  "padding-bottom": "20px"
                }
              },
              "@media (max-width: 767px)": {
                "&:not(.last-child)": {
                  "padding-bottom": "20px"
                }
              },
              "& when (@showFavorite)": {
                "&.favoritePrice": {
                  ".card-wrapper:before": {
                    "content": "'BEST'",
                    "position": "absolute",
                    "right": "-25px",
                    "top": "10px",
                    "font-size": "12px",
                    "font-weight": "600",
                    "-webkit-transform": "rotate(45deg)",
                    "transform": "rotate(45deg)",
                    "background-color": "@primaryColor",
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%)",
                    "padding": "5px 30px",
                    "line-height": "initial",
                    "z-index": "1"
                  },
                  ".card-head": {
                    "background-color": "@favoriteBgColor",
                    "> *, .amp-iconfont": {
                      "color": "@favoriteTextColor !important"
                    }
                  }
                },
                "&:not(.favoritePrice) .card-wrapper when (@showEffect) and not (@cardsAmount = '1')": {
                  ".card-head > *, .card-head, .amp-iconfont, svg": {
                    "-webkit-transition": "all 0.2s",
                    "transition": "all 0.2s"
                  },
                  "&:hover .card-head": {
                    "background-color": "@favoriteBgColor",
                    "> *, .amp-iconfont": {
                      "color": "@favoriteTextColor !important"
                    },
                    "svg": {
                      "fill": "@favoriteTextColor"
                    }
                  }
                }
              }
            },
            "& when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div": {
                "padding-left": "0",
                "padding-right": "0"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".title .milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "20px"
                },
                ".milestone > div:not(:last-child)": {
